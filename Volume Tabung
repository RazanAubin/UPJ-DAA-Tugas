	Algoritma
		1. Mendeklarasikan: 
     	 	 volume, luas, jari-jari, tinggi, dan pi

		  2. Masukan: 
        	 jari-jari = 3, tinggi = 5

		  3. Menghitung: 
        	 luas = pi*jari-jari*jari-jari

		  4. Menghitung: 
        	 volume = luas*tinggi

		  5. Menampilkan volume

	Pseudocode

		Deklarasi

	  	var volume, luas, jari-jari, tinggi, pi

		jari-jari = 3
		tinggi = 5
  
		luas = pi*jari-jari*jari-jari

	  	volume = luas*tinggi

	  	write(volume)

	    	return 0
